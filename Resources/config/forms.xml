<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">


    <services>

        <!-- Forms -->
        <service id="jfx_user_preferences.preferences.form"
            class="Zapoyok\UserPreferencesBundle\Form\Type\PreferencesType">
            <tag name="form.type" alias="jfx_user_preferences_preferences_form" />
            <argument>%jfx_user_preferences.preferences%</argument>
        </service>

    </services>
</container>
